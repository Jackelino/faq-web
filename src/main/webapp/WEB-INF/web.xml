    <servlet-class>mx.uaememx.Prueba</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Prueba</servlet-name>
    <url-pattern>/Prueba</url-pattern>
  </servlet-mapping>
  <servlet>
    <description>Servelet para consultar los temas.</description>
    <display-name>ConsultaTemas</display-name>
    <servlet-name>ConsultaTemas</servlet-name>
    <servlet-class>mx.uaememx.ConsultaTemas</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ConsultaTemas</servlet-name>
    <url-pattern>/getTemas</url-pattern>
  </servlet-mapping>
</web-app>